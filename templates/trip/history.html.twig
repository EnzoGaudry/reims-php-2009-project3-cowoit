{% extends 'base.html.twig' %}

{% block title %}History{% endblock %}

{% block body %}
<div>
   <h1>Historique</h1>

   <div>
      {% for trip in history %}
         <li>
            <p> <span>Départ :</span> {{ trip.addressStart }}</p>
            <p><span>Arrivée :</span> {{ trip.addressEnd }}</p>
            <p><span>Date :</span> {{ trip.date }}</p>
         </li>
      {% endfor %}
   </div>
</div>

{% endblock %}
